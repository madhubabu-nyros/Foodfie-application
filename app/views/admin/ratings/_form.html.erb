<div class="form-group">   
  <label>Rating</label>   
  <%= f.text_field :number, :class => "form-control", :placeholder => 'Enter Rating' %>
    <span class="err"><%= @rating.errors[:number].join('. ') %></span>   
</div>
<div class="form-group row">
  <div class="text-left"> 
    <%= link_to 'Back', admin_ratings_path, :class => "btn btn-warning" %> 
  </div>
  <div class="text-right">  
    <%= f.submit 'Save', :class => "btn btn-info" %>   
  </div>
</div>